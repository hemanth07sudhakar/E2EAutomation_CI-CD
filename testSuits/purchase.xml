<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"><?xml-model href="file.xsd" type="application/xml" schematypens="http://www.w3.org/2001/XMLSchema"?>
<!-- Test runs inp parallely-->
<suite name="Suite">
<groups>
	<run>
		<include name="purchase" />
	</run>
</groups>
  <test thread-count="5" name="Login Test">
    <classes>
      <class name="e_CommerceApp.test.MainTestFileTest"/>
    </classes>
  </test> 

<test thread-count="5" name="Product Test">
    <classes>
      <class name="e_CommerceApp.test.ErrorMessageTest"/>
    </classes>
  </test>
</suite> 
